    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const searchInput = document.getElementById('search-input');
            const searchButton = document.getElementById('search-button');
            const categorySelect = document.getElementById('category-select');
            const subcategorySelect = document.getElementById('subcategory-select');
            const videosContainer = document.querySelector('.videos');

            // Fonction pour charger les vidéos en fonction de la recherche
            function loadVideos() {
                const searchQuery = searchInput.value.toLowerCase();
                const selectedCategory = categorySelect.value;
                const selectedSubcategory = subcategorySelect.value;

                // Afficher ou masquer les vidéos en fonction de la recherche et des filtres de catégorie/sous-catégorie
                document.querySelectorAll('.video').forEach(video => {
                    const title = video.querySelector('h2').textContent.toLowerCase();
                    const category = video.classList.contains(selectedCategory);
                    const subcategory = video.classList.contains(selectedSubcategory);
                    const matchesSearch = title.includes(searchQuery);

                    if (matchesSearch && category && subcategory) {
                        video.style.display = 'block';
                    } else {
                        video.style.display = 'none';
                    }
                });
            }

            // Écouteurs d'événements pour la recherche et les filtres de catégorie/sous-catégorie
            searchInput.addEventListener('input', function () {
                loadVideos();
            });

            searchButton.addEventListener('click', function () {
                loadVideos();
            });

            categorySelect.addEventListener('change', function () {
                loadVideos();
            });

            subcategorySelect.addEventListener('change', function () {
                loadVideos();
            });

            // Chargement initial des vidéos
            loadVideos();
        });
    </script>
    <footer>
        <p><center>&copy; 2024 Mery Merco</center></p>
    </footer>
</body>
</html>
